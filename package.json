{
  "name": "billsplit",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "migrate": "bun scripts/migrate.ts",
    "build:with-migrations": "react-router build && bun run migrate",
    "dev": "portless splitsend bunx --bun react-router dev",
    "start": "bunx --bun react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "check": "ultracite check",
    "fix": "ultracite fix",
    "test": "playwright test",
    "test:ui": "playwright test --ui"
  },
  "dependencies": {
    "@knadh/oat": "^0.3.0",
    "@react-router/node": "7.12.0",
    "@react-router/serve": "7.12.0",
    "@tanstack/react-hotkeys": "^0.1.0",
    "currency.js": "^2.0.4",
    "isbot": "^5.1.31",
    "libsql": "^0.5.22",
    "lucide-react": "^0.564.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router": "7.12.0",
    "sileo": "^0.1.3",
    "unique-names-generator": "^4.7.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@react-router/dev": "7.12.0",
    "@types/bun": "^1.3.9",
    "@types/node": "^22",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "agentation": "^2.2.1",
    "oxfmt": "^0.33.0",
    "oxlint": "^1.48.0",
    "typescript": "^5.9.2",
    "ultracite": "^7.2.3",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
